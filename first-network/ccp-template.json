{
    "name": "first-network-org${ORG}",
    "version": "1.0.0",
    "client": {
        "organization": "Org${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "Org${ORG}": {
            "mspid": "Org${ORG}MSP",
            "peers": [
                "peer0.org${ORG}.testnet.svc.cluster.local",
                "peer1.org${ORG}.testnet.svc.cluster.local"
            ],
            "certificateAuthorities": [
                "ca.org${ORG}.testnet.svc.cluster.local"
            ]
        }
    },
    "peers": {
        "peer0.org${ORG}.testnet.svc.cluster.local": {
            "url": "grpcs://localhost:${P0PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.org${ORG}.testnet.svc.cluster.local",
                "hostnameOverride": "peer0.org${ORG}.testnet.svc.cluster.local"
            }
        },
        "peer1.org${ORG}.testnet.svc.cluster.local": {
            "url": "grpcs://localhost:${P1PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.org${ORG}.testnet.svc.cluster.local",
                "hostnameOverride": "peer1.org${ORG}.testnet.svc.cluster.local"
            }
        }
    },
    "certificateAuthorities": {
        "ca.org${ORG}.testnet.svc.cluster.local": {
            "url": "https://localhost:${CAPORT}",
            "caName": "ca-org${ORG}",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
